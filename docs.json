{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Funtend Docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Introduction",
        "pages": [
          "introduction",
          "introduction/problem",
          "introduction/solution",
          "introduction/tokenomics"
        ]
      },
      {
